   "source": [
    "nums = [1, 2, 3, 4]\n",
    "output = []\n",
    "def rec(i, stack):\n",
    "    if i == len(nums):\n",
    "        output.append(stack.copy())\n",
    "        return\n",
    "    stack.append(nums[i])\n",
    "    rec(i + 1, stack)\n",
    "    stack.pop()\n",
    "    rec(i + 1, stack)\n",
    "rec(0, [])\n",
    "# print(len(output))\n",
    "output"
   ]
  }
